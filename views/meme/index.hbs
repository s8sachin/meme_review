{{> partials/header nav="meme_list"}}
<div class="container">
  <div class="row" style="overflow-x: auto">
    <table class="table table table-responsive table-hover table-striped table-bordered">
      <thead>
        <th>Meme</th>
        <th>Name</th>
        <th>Pewds Ep no.</th>
        <th>Pewds Ep url</th>
        <th>Pewds Rating</th>
      </thead>
      <tbody>
        {{#if memes}}
          {{#forEach memes}}
            <tr>
              <td><img src="{{image_url}}" style="max-height: 50px"></td>
              <td>{{name}}</td>
              <td>{{meme_review_episode_num}}</td>
              <td><a href="{{meme_review_episode_url}}">{{meme_review_episode_url}}</a></td>
              <td>{{pewds_rating}}</a></td>
            </tr>
          {{/forEach}}
        {{/if}}
      </tbody>
    </table>
  </div>
</div>
